<h1>Listing pagetypes</h1>

<table>
  <tr>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pagetypes.each do |pagetype| %>
  <tr>
    <td><%= pagetype.type %></td>
    <td><%= link_to 'Show', pagetype %></td>
    <td><%= link_to 'Edit', edit_pagetype_path(pagetype) %></td>
    <td><%= link_to 'Destroy', pagetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pagetype', new_pagetype_path %>
